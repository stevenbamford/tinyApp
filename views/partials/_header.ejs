  <% if (email) { %>
<form id="username" method="POST" action="/logout">
    <input id="username" name="username" type="text" placeholder="Username" value="<%= email %>">
    <button class="edit" type="submit">Logout</button>
</form>
<button class="edit"><a href="/urls">My Links</a></button>
  <% } else { %>
  <button class="edit"><a class="edit" href="http://localhost:8080/login">Login</a></button>
  <button class="edit"><a class="edit" href="http://localhost:8080/register">Create account</a></button>

<% } %>


